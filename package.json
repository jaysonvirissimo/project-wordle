{
  "name": "verbula",
  "version": "0.2.0",
  "private": true,
  "author": "Jayson Virissimo <jayson.virissimo@asu.edu> and Josh Comeau <me@joshwcomeau.com>",
  "dependencies": {
    "eslint": "8.49.0",
    "eslint-config-react-app": "7.0.1",
    "new-component": "5.0.2",
    "parcel": "2.12.0",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "5.0.1"
  },
  "scripts": {
    "predev": "npm run build:dictionary && rimraf .parcel-cache dist",
    "dev": "parcel public/index.html",
    "build": "npm run build:dictionary && parcel build public/index.html",
    "build:production": "npm run build:dictionary && parcel build public/index.html --public-url /verbula/",
    "build:dictionary": "node scripts/build-dictionary.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "new-component": "new-component"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2"
  }
}
